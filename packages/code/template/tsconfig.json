{
  "compilerOptions": {
    "target": "es5",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "types": ["node"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "./src/**/*",
    
    "./scripts/*"
  ]
}
